---
import type { HTMLAttributes } from 'astro/types'
import { mergeStyle } from 'molcss'
import * as styles from './Pre.style'

type Props = HTMLAttributes<'pre'>

const props = {
  ...Astro.props,
  class: mergeStyle(styles.pre, Astro.props.class),
  style: undefined,
}
---

<pre {...props}><slot /></pre>
