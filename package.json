{
  "name": "tech-sandbox",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:copy && npm run build:script",
    "build:copy": "cp -r ./public/* ./dist",
    "build:script": "node ./scripts",
    "build-prod": "npm test && npm run clean && NODE_ENV=production npm run build",
    "test": "npm run test:textlint",
    "test:textlint": "textlint ./content",
    "clean": "rm -r dist/*",
    "serve": "serve dist -p 3000",
    "gh-pages": "gh-pages -d dist -b master",
    "deploy": "npm run build-prod && npm run gh-pages"
  },
  "dependencies": {
    "hyperapp": "^1.2.9"
  },
  "devDependencies": {
    "@hyperapp/render": "^2.1.0",
    "@types/html-minifier": "^3.5.2",
    "@types/marked": "^0.5.0",
    "@types/mkdirp": "^0.5.2",
    "@types/page": "^1.8.0",
    "@types/prismjs": "^1.9.0",
    "critical": "^1.3.4",
    "gh-pages": "^2.0.1",
    "html-minifier": "^3.5.21",
    "jdown": "^0.6.6",
    "marked": "^0.5.2",
    "mkdirp": "^0.5.1",
    "prismjs": "^1.15.0",
    "rollup": "^0.68.1",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-postcss": "^1.6.3",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-watch": "^4.3.1",
    "serve": "^10.1.1",
    "textlint": "^11.0.1",
    "textlint-rule-preset-japanese": "^4.0.0",
    "textlint-rule-preset-jtf-style": "^2.3.1",
    "tslib": "^1.9.3",
    "typescript": "3.1.x"
  }
}
