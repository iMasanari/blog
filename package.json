{
  "name": "tech-sandbox",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "npm run build:react-static && npm run build:uglifyjs",
    "build:react-static": "react-static build",
    "build:uglifyjs": "uglifyjs dist/static.*.js -o `find dist -name static.\\*.js`",
    "serve": "serve dist -p 3000",
    "gh-pages": "gh-pages -d dist -b master"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-ga": "^2.5.3",
    "react-static": "^5.9.7"
  },
  "devDependencies": {
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-router": "^4.0.16",
    "@types/webpack-env": "^1.13.5",
    "eslint-config-react-tools": "1.1.2",
    "gh-pages": "^1.2.0",
    "jdown": "^0.6.4",
    "marked": "^0.4.0",
    "prismjs": "^1.15.0",
    "serve": "^6.5.3",
    "ts-loader": "^3.5.0",
    "typescript": "^2.7.2",
    "uglifyjs": "^2.4.11"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
